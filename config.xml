<?xml version="1.0" encoding="UTF-8"?>

<widget xmlns     = "http://www.w3.org/ns/widgets"
		xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.designge.pensarecasa"
	    versionCode="11" 
        version   = "1.0.1"
		package="com.designge.pensarecasa"
		xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Pensarecasa</name>


	<icon src="logo.png" />


<access origin="*" />
<allow-intent href="*" />
<allow-navigation href="*" />
<allow-navigation href="*://*.pensarecasa.design.ge/*" />
 <platform name="android">
        <preference name="android-minSdkVersion" value="18" />
        <allow-intent href="market:*" />
    </platform>


<preference name="android-build-tool" value="gradle" /> 
<!--<preference name="StatusBarOverlaysWebView" value="true" />-->
	<preference name="phonegap-version" value="cli-8.0.0" /> 
	<preference name="orientation" value="portrait" />
	<preference name="target-device"              value="handset" />
<!--	<preference name="fullscreen" value="true" />-->
	<preference name="webviewbounce" value="true" />  
	<preference name="DisallowOverscroll" value="false" />
	<preference name="stay-in-webview"            value="false" /> 

<preference name="exit-on-suspend"            value="false" />          
<preference name="show-splash-screen-spinner" value="false" />    
<preference name="auto-hide-splash-screen"    value="false" />
<preference name="disable-cursor"             value="false" />
<preference name="android-minSdkVersion"      value="11" />         
<preference name="android-installLocation"    value="auto" />
<preference name="EnableViewportScale"    value="true" />

<preference name="StatusBarOverlaysWebView" value="true" />
<preference name="StatusBarBackgroundColor" value="#FFFFFF" />
<preference name="StatusBarStyle" value="styleDefault" />

	<description>
Pensarecasa
    </description>
	

    <author href="http://design.ge" email="uabashidze@gmail.com">
        Cyborg-Mcloud
    </author>


 
    <plugins>

    </plugins>



	<feature name="App">
	  <param name="android-package" value="com.designge.pensarecasa"/>
	</feature>
	
<gap:plugin name="cordova-plugin-statusbar" source="npm" />
	<gap:plugin name="cordova-plugin-inappbrowser"  source="npm" />
	<gap:plugin name="cordova-plugin-whitelist" source="npm" />
	<feature name="http://api.phonegap.com/1.0/file" />


	<gap:plugin name="cordova-plugin-inappbrowser"  source="npm" />

</widget>
